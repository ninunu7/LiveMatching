<h1>検索する</h1>
<div>
<<%= form_with url: searches_path, method: :get, local: true do |f| %>
  <%= f.text_field :artist_name %>
  <%= f.submit :search %>
<% end %>

</div>
    <%= f.submit('検索') %>
    <% end %>

<h1>検索結果</h1>
<% @search.each do |search| %>
      <tr>
        <td><%= search.artist_name %></td>
      </tr>
    <% end %>

